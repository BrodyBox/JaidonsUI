game:GetService("RunService").RenderStepped:Connect(function()
    local args = {
    [1] = "I am an idiot!",
    [2] = "All"
}
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
local args = {
    [1] = "☻ ☻ ☻",
    [2] = "All"
}
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
wait()
end)
